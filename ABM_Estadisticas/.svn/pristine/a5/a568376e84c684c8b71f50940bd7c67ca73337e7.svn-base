<?php

namespace abmestadisticas\abmBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Subtitulo
 *
 * @ORM\Table()
 * @ORM\Entity
 */
class Subtitulo
{
    /**
     * @var integer
     *
     * @ORM\Column(name="subt_id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $subt_id;

    /**
     * @var integer
     *
     * @ORM\Column(name="subt_capitulo", type="integer")
     */
    private $subt_capitulo;

    /**
     * @var integer
     *
     * @ORM\Column(name="subt_titulo", type="integer")
     */
    private $subt_titulo;

    /**
     * @var string
     *
     * @ORM\Column(name="subt_detalle", type="string", length=255)
     */
    private $subt_detalle;


    /**
     * Get subt_id
     *
     * @return integer 
     */
    public function getSubtId()
    {
        return $this->subt_id;
    }

    /**
     * Set subt_capitulo
     *
     * @param integer $subtCapitulo
     * @return Subtitulo
     */
    public function setSubtCapitulo($subtCapitulo)
    {
        $this->subt_capitulo = $subtCapitulo;

        return $this;
    }

    /**
     * Get subt_capitulo
     *
     * @return integer 
     */
    public function getSubtCapitulo()
    {
        return $this->subt_capitulo;
    }

    /**
     * Set subt_titulo
     *
     * @param integer $subtTitulo
     * @return Subtitulo
     */
    public function setSubtTitulo($subtTitulo)
    {
        $this->subt_titulo = $subtTitulo;

        return $this;
    }

    /**
     * Get subt_titulo
     *
     * @return integer 
     */
    public function getSubtTitulo()
    {
        return $this->subt_titulo;
    }

    /**
     * Set subt_detalle
     *
     * @param string $subtDetalle
     * @return Subtitulo
     */
    public function setSubtDetalle($subtDetalle)
    {
        $this->subt_detalle = $subtDetalle;

        return $this;
    }

    /**
     * Get subt_detalle
     *
     * @return string 
     */
    public function getSubtDetalle()
    {
        return $this->subt_detalle;
    }
}
