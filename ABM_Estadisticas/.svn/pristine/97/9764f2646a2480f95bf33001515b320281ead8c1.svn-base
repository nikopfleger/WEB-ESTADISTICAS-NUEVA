					$("#info").empty();
					$("#info").append("Operaci√≥n exitosa");
					$("#info").dialog("open");
				},
				error: function(xhr, textStatus, errorThrown) {
					alert("ERROR: " + textStatus);
                	return false;
				}
			});
	
		}
		
		
		
	});
	
