		$("#ares_aresTitulo").trigger("change");
		$("#ares_aresLink").val("");
		$("#ares_aresDescarga").val("");
		$("#ares_aresDescripcion").val("");
		$("#ares_aresComentario").val("");
		$("#ares_aresEditar").val(0);
